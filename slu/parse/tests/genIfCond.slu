impl Out {
	ex fn genIfCond(&mut self,
		itm = &Self::IfCond(isExpr),
		const isExpr = bool
	) {
		self.add("if ");
		self.genExpr(&itm.cond);
		self.genSoe(&itm.bl,true);
		
		if isExpr => self.add(' ');
		
		for {&expr,&bl} in itm.elseIfs
		{
			self.add("else if ");
			
			self.genExpr(expr);
			self.genSoe(bl,true);
			if isExpr => self.add(' ');
		}
		for &bl in itm.elseBlock --Option is a list of 0..1 elements.
		{
			self.add("else");
			self.genSoe(bl,false);
		}
	}
}